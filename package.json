{
  "name": "servertool",
  "version": "1.0.1",
  "description": "Tools for Build Your Server",
  "main": "build/app.js",
  "scripts": {
    "start": "electron .",
    "install": "npm i && npx tsc --build && npx electron-rebuild -f -o node-pty",
    "buildscript": "tsc --build",
    "buildwin": "npx electron-packager . ServerTool --overwrite --asar --platform=win32 --arch=ia32 --icon=build/img/terminal.ico --prune=true --out=releasebuilds --version-string.CompanyName=XyZerKunG --version-string.FileDescription=XyZerKunG --version-string.ProductName=\"ServerTool\"",
    "buildmac": "npx electron-packager . --overwrite --platform=darwin --arch=x64 --icon=build/img/terminal.icns --prune=true --out=releasebuilds",
    "buildlinux": "npx electron-packager . ServerTool --overwrite --asar --platform=linux --arch=x64 --icon=build/img/terminal-x1024.png --prune=true --out=releasebuilds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XyZerKunG/ServerTool.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "XyZerKunG",
  "license": "CC0-1.0",
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "electron": "^12.0.4",
    "electron-packager": "^15.2.0",
    "electron-rebuild": "^2.3.5",
    "typescript": "^4.2.4"
  },
  "bugs": {
    "url": "https://github.com/XyZerKunG/ServerTool/issues"
  },
  "homepage": "https://github.com/XyZerKunG/ServerTool#readme",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/open": "^6.2.1",
    "@types/request": "^2.48.5",
    "axios": "^0.21.1",
    "child_process": "^1.0.2",
    "json-stringify-pretty-compact": "^3.0.0",
    "line-reader": "^0.4.0",
    "node-fetch": "^2.6.1",
    "node-gyp": "^8.0.0",
    "node-pty": "^0.10.1",
    "open": "^8.0.6",
    "request": "^2.88.2",
    "sweetalert2": "^10.16.3",
    "xterm": "^4.12.0",
    "xterm-addon-attach": "^0.6.0",
    "xterm-addon-fit": "^0.5.0",
    "xterm-addon-search": "^0.8.0",
    "xterm-addon-unicode11": "^0.2.0",
    "xterm-addon-web-links": "^0.4.0",
    "yauzl": "^2.10.0"
  }
}
